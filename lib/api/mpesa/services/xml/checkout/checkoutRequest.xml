        <tns:CheckOutHeader>
            <MERCHANT_ID></MERCHANT_ID>
            <REFERENCE_ID></REFERENCE_ID>
            <PASSWORD></PASSWORD>
            <TIMESTAMP></TIMESTAMP>
        </tns:CheckOutHeader>
    </soapenv:Header>
