        <tns:CheckOutHeader>
            <MERCHANT_ID></MERCHANT_ID>
            <PASSWORD></PASSWORD>
            <TIMESTAMP></TIMESTAMP>
        </tns:CheckOutHeader>
    </soapenv:Header>
    <soapenv:Body>
        <tns:transactionStatusRequest>
            <!--Optional:-->
            <TRX_ID></TRX_ID>
